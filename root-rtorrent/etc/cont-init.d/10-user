#!/usr/bin/with-contenv bash

PUID=${PUID:-911}
PGID=${PGID:-911}

groupmod --non-unique --gid "$PGID" abc
usermod  --non-unique --uid "$PUID" abc

echo "
-------------------------------------
GID/UID
-------------------------------------"
echo "
User uid:    $(id -u abc)
User gid:    $(id -g abc)
-------------------------------------"